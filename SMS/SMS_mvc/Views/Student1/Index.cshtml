@model IEnumerable<Student>
<h1> Student Page</h1>

<table class="table">
    @foreach(var item in Model)
    {
<tr>
    <td>@item.StudentId</td>
    <td>@item.FName</td>
    <td>@item.LName </td>
    <td>@item.Gender</td>
    <td> <button class="btn btn-danger" Edit </button >  </td>
</tr>
    }
</table>


